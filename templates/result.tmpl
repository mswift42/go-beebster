<div class="rtable">
  <h2 class="header">Results</h2>
  {{range .results}}
  {{if .Oldrecordings}}
  <br/>
  <div class="header">
    <p>{{.Oldrecordings}}</p>
  </div>
</div>
  {{end}}

{{if .Title}}
  <div class="table">
    <div class="tablecell">
      <div class="t1">
        <a href={{.Index}}>
        <img class="img" src={{.Thumbnail}} alt={{.Title}}/>
        </a>
      </div>
      <div class="imgtitle">
        <p>{{.Title}}</p>
      </div>
    </div>
  </div>
  {{end}}
  {{end}}
</div>
