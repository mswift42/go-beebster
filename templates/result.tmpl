<div class="rtable">
  {{range .}}
  <div class="table">
    <div class="tablecell">
      <div class="t1">
        <a href={{.Index}}>{{.Title}}</a>
        <img class="img" src={{.Thumbnail}} alt={{.Title}}/>
      </div>
      <div class="imgtitle">
        {{.Title}}
      </div>
    </div>
  </div>
  {{end}}
</div>
