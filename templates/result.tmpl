<div class="rtable">
  {{range .}}
  <div class="table">
    <div class="tablecell">
      <div class="t1">
        <a href={{.Index}}>
        <img class="img" src={{.Thumbnail}} alt={{.Title}}/>
        </a>
      </div>
      <div class="imgtitle">
        {{.Title}}
      </div>
    </div>
  </div>
  {{end}}
</div>
